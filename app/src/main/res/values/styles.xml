<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="ToolBarStyle" parent="ThemeOverlay.AppCompat.ActionBar">
        <item name="android:textColorPrimary">@color/appBarText</item>
        <item name="android:textColorSecondary">@color/appBarText</item>
        <item name="android:actionMenuTextColor">@color/appBarText</item>
    </style>

</resources>
